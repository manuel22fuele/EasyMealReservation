{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",

  "scripts": {
    "postinstall": "yarn prisma:generate",
    "start": "node ./build/server.js",
    "dev": "ts-node-dev src/server.ts",
    "build": "tsc",
    "prisma:migrate": "prisma migrate dev --schema=./src/config/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=./src/config/prisma/schema.prisma",
    "prisma:deploy": "npx prisma migrate deploy --schema=./src/config/prisma/schema.prisma"

  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "prisma": "^5.7.1",
    "zod": "^3.22.4"
  }
}
